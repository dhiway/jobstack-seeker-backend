    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.4.17",
    "@types/node": "^25.0.9",
    "@types/nodemailer": "^7.0.5",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.31.8",
    "tsx": "^4.21.0",
    "typescript": "^5.9.2",
    "vite": "^7.3.1"
  }
