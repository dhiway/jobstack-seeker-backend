{
  "name": "Jobstack Seeker Backend",
  "version": "1.0.0",
  "description": "API Service for Auth, Seeker Backend and Beckn Layer",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "build": "vite build",
    "auth:generate": "pnpm dlx @better-auth/cli generate --config ./src/lib/auth/index.ts",
    "db:pull": "drizzle-kit pull",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "test": "echo \"No tests defined\" && exit 0"
  },
  "keywords": [
    "JobStack Seeker",
    "JobStack Auth"
  ],
  "author": "Dhiway",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "@aws-sdk/client-sesv2": "^3.873.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/env": "^5.0.2",
    "@fastify/formbody": "^8.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.5.1",
    "@scalar/fastify-api-reference": "^1.34.6",
    "@sendgrid/mail": "^8.1.5",
    "better-auth": "^1.3.7",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.5",
    "fastify": "^5.5.0",
    "fastify-plugin": "^5.0.1",
    "fastify-type-provider-zod": "^5.0.3",
    "highlight.js": "^11.11.1",
    "ioredis": "^5.7.0",
