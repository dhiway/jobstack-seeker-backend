{
  "name": "Jobstack Seeker Backend",
  "version": "1.0.1",
  "description": "API Service for Auth, Seeker Backend and Beckn Layer",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "build": "vite build",
    "auth:generate": "pnpm dlx @better-auth/cli generate --config ./src/lib/auth/index.ts",
    "db:pull": "drizzle-kit pull",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "session:query": "tsx scripts/sessionActivityQueries.ts",
    "test": "echo \"No tests defined\" && exit 0"
  },
  "keywords": [
    "JobStack Seeker",
    "JobStack Auth"
  ],
  "author": "Dhiway",
  "license": "MIT",
  "packageManager": "pnpm@10.28.0",
  "dependencies": {
    "@aws-sdk/client-sesv2": "^3.971.0",
    "@better-auth/sso": "^1.4.17",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.2.0",
    "@fastify/env": "^5.0.2",
    "@fastify/formbody": "^8.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.6.1",
    "@google-cloud/storage": "^7.18.0",
    "@scalar/fastify-api-reference": "^1.43.8",
    "better-auth": "^1.4.17",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.45.1",
    "fastify": "^5.7.3",
    "fastify-plugin": "^5.0.1",
    "fastify-type-provider-zod": "^6.1.0",
    "highlight.js": "^11.11.1",
    "ioredis": "^5.9.2",
    "nodemailer": "^7.0.12",
