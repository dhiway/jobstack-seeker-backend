  "devDependencies": {
    "@better-auth/cli": "^1.3.7",
    "@types/node": "^24.3.0",
    "@types/nodemailer": "^7.0.1",
    "@types/pg": "^8.15.5",
    "drizzle-kit": "^0.31.4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  }
